<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gec.dao.AuctionMapper">
    <select id="getAuctionList" resultType="com.gec.pojo.Auction">
        select  *  from auction
    </select>
    <select id="getAuctionByAuctionid" resultType="com.gec.pojo.Auction">
         select  *  from auction where auctionid = #{auctionid}
    </select>
</mapper>